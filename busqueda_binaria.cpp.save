int FnIntBusquedaBinaria(int lista[], int dato)
{
    int izquierda, derecha, medio;
    izquierda = 0;
    derecha = sizeof(lista) / 4;
    while(izquierda <= derecha)
    {
        medio = (izquierda+derecha)/2;
        if(dato == lista[medio])
            return medio;
        else if(dato < lista[medio])
            derecha = medio - 1;
        else
            izquierda = medio + 1;
    }
    return false;
}

